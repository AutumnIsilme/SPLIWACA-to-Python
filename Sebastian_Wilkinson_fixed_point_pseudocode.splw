REQUIRE python

OUTPUT "Enter a base 10 number: "
INPUT float base10
SET integer_part TO CALL math.floor WITH base10
SET integer_part TO CALL _interpreter.bin WITH integer_part
SET float_part TO base10 - integer_part
SET after_point TO ""
FOR CALL _interpreter.range WITH 8 DO
	SET float_part TO float_part * 2
	IF float_part > 1 DO
		SET float_part TO float_part - 1
		SET after_point TO after_point + "1"
	ELSE DO
		SET after_point TO after_point + "0"
	END IF
END FOR

