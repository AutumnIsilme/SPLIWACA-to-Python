REQUIRE transpiler_py

IMPORT time

FUNCTION Main RETURNS int AS
	OUTPUT Enter a number: 
	INPUT INT n
<<<<<<< Updated upstream
<<<<<<< Updated upstream
	OUTPUT
=======
	OUTPUT \n
>>>>>>> Stashed changes
=======
	OUTPUT \n
>>>>>>> Stashed changes
	
	SET sumSequential TO CREATE INT
	
	SET timeStartSequential TO CALL _INTERPRETER.time.time
	
	FOR i OF (CALL _INTERPRETER.range WITH n) DO
		SET sumSequential TO sumSequential + i + 1
	END FOR
	
	SET timeEndSequential TO CALL _INTERPRETER.time.time
	
	SET timeStartGaussian TO CALL _INTERPRETER.time.time
	
	SET sumGaussian TO n * ( n + 1 ) * 0.5
	
	SET timeEndGaussian TO CALL _INTERPRETER.time.time
	
	IF sumGaussian != sumSequential DO
		OUTPUT Sums are not equal. Something is wrong! Gaussian result: $sumGaussian , sequential result: $sumSequential
		SET rv TO -1
	ELSE DO
		SET differenceSequential TO timeEndSequential - timeStartSequential
		SET differenceGaussian TO timeEndGaussian - timeStartGaussian
		
		SET strSumGaussian TO CAST STR sumGaussian
		OUTPUT The sum is: $strSumGaussian

		SET strTimeSequential TO CAST STR differenceSequential
		OUTPUT Time sequential: $strTimeSequential
		SET strTimeGaussian TO CAST STR differenceGaussian
		OUTPUT Time gaussian: $strTimeGaussian
		
		SET rv TO 0
	END IF
	RETURN rv
END FUNCTION

CALL Main