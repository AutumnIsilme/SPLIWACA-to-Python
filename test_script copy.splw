REQUIRE python

OUTPUT "Enter a base 10 number: "
INPUT float base10
SET integer_part TO CALL _INTERPRETER.math.floor WITH base10
SET float_part TO base10 - integer_part
SET integer_part TO (CALL _INTERPRETER.bin WITH integer_part)
SET integer_part TO CALL integer_part.substr WITH 2, 0
SET after_point TO ""
FOR i OF (CALL _INTERPRETER.range WITH 8) DO
	SET float_part TO float_part * 2
	IF float_part > 1 DO
		SET float_part TO float_part - 1
		SET after_point TO after_point + "1"
	ELSE DO
		SET after_point TO after_point + "0"
	END IF
END FOR

OUTPUT $integer_part . $after_point